{
  "name": "paygate-sdk",
  "version": "0.2.0",
  "description": "Typescript and Javascript utilities and API client to aid with PayGate integration. Optional ExpressJS middleware to simplify custom payment endpoints.",
  "homepage": "https://github.com/distributhor/paygate-sdk",
  "license": "MIT",
  "keywords": [
    "paygate"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/distributhor/paygate-sdk.git"
  },
  "contributors": [
    "WK <distributhor@fastmail.com>"
  ],
  "files": [
    "LICENSE.md",
    "README.md",
    "/lib/paygate.js",
    "/dist/browser.d.ts",
    "/dist/browser.d.ts.map",
    "/dist/browser.js",
    "/dist/browser.js.map",
    "/dist/client.d.ts",
    "/dist/client.d.ts.map",
    "/dist/client.js",
    "/dist/client.js.map",
    "/dist/index.d.ts",
    "/dist/index.d.ts.map",
    "/dist/index.js",
    "/dist/index.js.map",
    "/dist/middleware.d.ts",
    "/dist/middleware.d.ts.map",
    "/dist/middleware.js",
    "/dist/middleware.js.map",
    "/dist/types.d.ts",
    "/dist/types.d.ts.map",
    "/dist/types.js",
    "/dist/types.js.map",
    "/dist/util.d.ts",
    "/dist/util.d.ts.map",
    "/dist/util.js",
    "/dist/util.js.map"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -b --clean && tsc -b && rollup -c rollup.config.js",
    "test": "jest -c test/jest.config.js --passWithNoTests",
    "lint": "yarn lint:check",
    "lint:check": "eslint '{src,test}/**/*.{js,ts,json}'",
    "lint:fix": "eslint --fix '{src,test}/**/*.{js,ts,json}'",
    "lint:format": "yarn prettier:fix",
    "prettier:fix": "prettier --ignore-path .prettierignore --write '{src,test}/**/*.{js,ts,json,md}'",
    "prettier:check": "prettier --ignore-path .prettierignore --check '{src,test}/**/*.{js,ts,json,md}'",
    "doc:html": "typedoc --tsconfig ./tsconfig.json --out docs/html --plugin typedoc-neo-theme --theme ./node_modules/typedoc-neo-theme/bin/default",
    "doc:markdown": "typedoc --tsconfig ./tsconfig.json --out docs/markdown --readme none --plugin typedoc-plugin-markdown",
    "doc": "yarn doc:html && yarn doc:markdown"
  },
  "dependencies": {
    "currency.js": "^2.0.3",
    "debug": "^4.2.0",
    "luxon": "^1.25.0",
    "md5": "^2.3.0",
    "node-cache": "^5.1.2",
    "qs": "^6.9.4",
    "superagent": "^6.1.0",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/luxon": "^1.25.0",
    "@types/md5": "^2.2.1",
    "@types/node": "^14.11.10",
    "@types/node-cache": "^4.2.5",
    "@types/qs": "^6.9.5",
    "@types/superagent": "^4.1.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.5.3",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2",
    "rollup": "^2.32.1",
    "rollup-plugin-typescript2": "^0.28.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typedoc": "^0.19.2",
    "typedoc-neo-theme": "^1.0.10",
    "typedoc-plugin-markdown": "^3.0.11",
    "typescript": "^4.0.3",
    "yarn": ">=1.22.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint && yarn test"
    }
  },
  "lint-staged": {
    "*.{js,json,md,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 120
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  }
}
